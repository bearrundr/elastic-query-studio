{
    "name": "elastic-query-studio",
    "displayName": "Elastic Query Studio",
    "description": "Elasticsearch query supporting DSL, ESQL, and EQL, SQL for Elasticsearch, OpenSearch and Elastic Cloud",
    "version": "0.5.25",
    "publisher": "daeyong-cloudin",
    "type": "commonjs",
    "icon": "media/eqs128x128.png",
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "yarn package",
        "package": "webpack --mode production --devtool hidden-source-map",
        "test": "mocha --require ts-node/register test/**/*.test.ts",
        "test:es": "mocha --require ts-node/register test/unit/es_connect.test.ts",
        "test:watch": "mocha --require ts-node/register test/**/*.test.ts --watch",
        "compile": "webpack",
        "watch": "webpack --watch",
        "dev": "cross-env NODE_ENV=development yarn watch",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
        "lint:check": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
        "deploy": "cross-env-shell \"vsce publish --yarn --githubBranch $BRANCH --baseContentUrl $REPO_URL --baseImagesUrl $REPO_URL --pat $VSCE_PAT\"",
        "postinstall": "husky install",
        "updateDep": "yarn upgrade-interactive --latest"
    },
    "engines": {
        "vscode": "^1.66.0"
    },
    "galleryBanner": {
        "color": "#005571",
        "theme": "dark"
    },
    "categories": [
        "Programming Languages",
        "Data Science",
        "Snippets"
    ],
    "keywords": [
        "Elasticsearch",
        "OpenSearch",
        "Elastic Cloud",
        "OpenSearch Cloud",
        "DSL",
        "ESQL",
        "EQL",
        "SQL",
        "Query",
        "Database"
    ],
    "tags": [
        "Elasticsearch",
        "OpenSearch",
        "Elastic Cloud",
        "OpenSearch Cloud",
        "DSL",
        "ESQL",
        "EQL",
        "SQL",
        "Elastic",
        "Kibana"
    ],
    "bugs": {
        "url": "https://github.com/bearrundr/elastic-query-studio/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bearrundr/elastic-query-studio.git"
    },
    "activationEvents": [
        "onLanguage:esql",
        "onCommand:elastic-query-studio.execute",
        "onCommand:elastic-query-studio.setHost"
    ],
    "contributes": {
        "commands": [
            {
                "command": "elastic-query-studio.setHost",
                "title": "Elasticsearch: Set Host",
                "category": "ES"
            },
            {
                "command": "elastic-query-studio.execute",
                "title": "Elasticsearch: Run Query",
                "category": "ES"
            }
        ],
        "languages": [
            {
                "id": "esql",
                "aliases": [
                    "ElasticSearch",
                    "esql",
                    "elasticsearch"
                ],
                "extensions": [
                    "esql"
                ],
                "configuration": "./grammar/es.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "esql",
                "scopeName": "source.esql",
                "path": "./grammar/es.tmLanguage"
            }
        ],
        "configuration": {
            "title": "Elasticsearch",
            "properties": {
                "elastic.showResultAsDocument": {
                    "type": "boolean",
                    "default": true,
                    "description": "Show result in a new json file, or show in default view"
                },
                "elastic.host": {
                    "type": "string",
                    "default": "http://localhost:9200",
                    "description": "Elasticsearch Host"
                }
            }
        },
        "keybindings": [
            {
                "key": "ctrl+enter",
                "command": "elastic-query-studio.execute",
                "when": "editorTextFocus && resourceLangId == 'esql'"
            },
            {
                "command": "elastic-query-studio.execute",
                "key": "alt+Enter",
                "when": "editorTextFocus && resourceLangId==esql"
            }
        ]
    },
    "dependencies": {
        "axios": "^0.26.1",
        "copy-paste": "^1.3.0",
        "cross-env": "^7.0.3",
        "json-markup": "^1.1.3",
        "jsonc-parser": "^3.0.0",
        "mustache": "^4.2.0",
        "path": "^0.12.7",
        "routington": "^1.0.3",
        "semver-closest": "^0.1.2"
    },
    "devDependencies": {
        "@types/chai": "^4.3.5",
        "@types/copy-paste": "^1.1.30",
        "@types/jsesc": "^3.0.3",
        "@types/mocha": "10.0.6",
        "@types/node": "20.11.30",
        "@types/require-dir": "^1.0.4",
        "@types/vscode": "^1.66.0",
        "@typescript-eslint/eslint-plugin": "^8.29.1",
        "@typescript-eslint/parser": "^8.29.1",
        "chai": "4.3.7",
        "del": "^6.0.0",
        "eslint": "^9.24.0",
        "eslint-config-prettier": "^10.1.2",
        "eslint-plugin-prettier": "^5.2.6",
        "husky": "^7.0.4",
        "jsesc": "^3.0.2",
        "mocha": "^9.2.2",
        "npm-check-updates": "^12.5.9",
        "prettier": "^2.6.2",
        "require-dir": "^1.2.0",
        "ts-loader": "^9.2.8",
        "ts-node": "10.9.2",
        "tsconfig-paths": "^4.2.0",
        "tslib": "^2.3.1",
        "typescript": "4.9.5",
        "vsce": "^2.7.0",
        "vscode-test": "^1.6.1",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2"
    }
}
